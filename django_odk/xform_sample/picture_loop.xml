<?xml version="1.0"?><h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema"><h:head><h:title>picture_loop</h:title><model odk:xforms-version="1.0.0"><instance><data id="picture_loop"><today/><deviceid/><phonenumber/><username/><loc><loc_web_on/><loc_xy/><loc_street_name/><loc_street_nr/><loc_location/><loc_zip/></loc><srv><srv_grouwth/><srv_comments/></srv><pic_nbr/><pic_count/><pic jr:template=""><pic_nbr_count/><pic_img/></pic><pic><pic_nbr_count/><pic_img/></pic><meta><instanceID/></meta></data></instance><bind jr:preload="date" jr:preloadParams="today" nodeset="/data/today" type="date"/><bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/data/deviceid" type="string"/><bind jr:preload="property" jr:preloadParams="phonenumber" nodeset="/data/phonenumber" type="string"/><bind jr:preload="property" jr:preloadParams="username" nodeset="/data/username" type="string"/><bind nodeset="/data/loc/loc_web_on" type="string"/><bind nodeset="/data/loc/loc_xy" relevant=" /data/loc/loc_web_on ='yes'" required="true()" type="geopoint"/><bind nodeset="/data/loc/loc_street_name" relevant=" /data/loc/loc_web_on ='no'" type="string"/><bind nodeset="/data/loc/loc_street_nr" relevant=" /data/loc/loc_web_on ='no'" type="int"/><bind nodeset="/data/loc/loc_location" relevant=" /data/loc/loc_web_on ='no'" type="string"/><bind nodeset="/data/loc/loc_zip" relevant=" /data/loc/loc_web_on ='no'" type="string"/><bind nodeset="/data/srv/srv_grouwth" type="string"/><bind nodeset="/data/srv/srv_comments" relevant=" /data/loc/loc_web_on ='no'" type="string"/><bind nodeset="/data/pic_nbr" type="int"/><bind calculate=" /data/pic_nbr " nodeset="/data/pic_count" readonly="true()" type="string"/><bind calculate="position(..)" nodeset="/data/pic/pic_nbr_count" type="string"/><bind nodeset="/data/pic/pic_img" orx:max-pixels="10000" type="binary"/><bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/></model></h:head><h:body><group ref="/data/loc"><label>LOCATION</label><select1 ref="/data/loc/loc_web_on"><label>Possible to get GPS coordinates?</label><item><label>yes</label><value>yes</value></item><item><label>no</label><value>no</value></item></select1><input ref="/data/loc/loc_xy"><label>GPS coordinates</label></input><input ref="/data/loc/loc_street_name"><label>Street name</label></input><input ref="/data/loc/loc_street_nr"><label>Number in the street</label></input><input ref="/data/loc/loc_location"><label>Location</label></input><input ref="/data/loc/loc_zip"><label>Postal code</label></input></group><group ref="/data/srv"><label>SURVEY</label><select1 ref="/data/srv/srv_grouwth"><label>Growth percentage</label><item><label>Sowed</label><value>S</value></item><item><label>0-60% growth</label><value>L1</value></item><item><label>60-85% growth</label><value>L2</value></item><item><label>&gt;85% growth</label><value>L3</value></item></select1><input ref="/data/srv/srv_comments"><label>Any comments?</label></input></group><input ref="/data/pic_nbr"><label>Number of pictures</label></input><group ref="/data/pic"><label>PICTURE</label><repeat jr:count=" /data/pic_count " nodeset="/data/pic"><upload mediatype="image/*" ref="/data/pic/pic_img"><label>Picture <output value=" ../pic_nbr_count "/></label></upload></repeat></group></h:body></h:html>